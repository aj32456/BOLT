async function startProgram() {

	setMainLed({ r: 0, g: 0, b: 255 });

	await speak("Hello Square2", true);

	await delay(1);

	for (var _i1 = 0; _i1 < 8; _i1++) {

		setMainLed(getRandomColor());

		await Sound.Animal.play(true);

		await roll((getHeading() + 90), 60, 1);

		await delay(1);

	}

}
